{
  "name": "satisfactory-mod-launcher-api",
  "version": "0.0.1",
  "description": "API to manage installed Satisfactory mods",
  "main": "build/index.js",
  "scripts": {
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "build:ts": "tsc",
    "build": "yarn lint && yarn build:ts",
    "watch:ts": "tsc -w",
    "debug": "nodemon --watch src/ -e ts --exec yarn serve || exit 1",
    "serve": "yarn build && node build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mircea Roata",
  "license": "GPL-3.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@types/jszip": "^3.1.6",
    "@types/request-promise-native": "^1.0.17",
    "jszip": "^3.2.2",
    "platform-folders": "^0.4.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8"
  }
}
